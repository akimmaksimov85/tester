{
  "name": "akimmaksimov85/tester",
  "description": "Clean Architecture Code Tester",
  "type": "symfony-bundle",
  "license": "proprietary",
  "minimum-stability": "dev",
  "require": {
    "php": ">=7.4",
    "ext-ctype": "*",
    "ext-iconv": "*",
    "symfony/console": "5.2.*",
    "symfony/dotenv": "5.2.*",
    "symfony/flex": "^1.3.1",
    "symfony/framework-bundle": "5.2.*",
    "symfony/yaml": "5.2.*",
    "fakerphp/faker": "^1.13",
    "ext-json": "*",
    "coduo/php-matcher": "^6.0",
    "haydenpierce/class-finder": "^0.4.0",
    "symfony/browser-kit": "^5.0",
    "phpunit/phpunit": "^10.0@dev",
    "symfony/messenger": "5.2.*"
  },
  "autoload": {
    "psr-4": {
      "Akimmaksimov85\\TesterBundle\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Akimmaksimov85\\TesterBundle\\Tests\\": "tests"
    }
  }
